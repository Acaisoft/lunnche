{
  "name": "angular2-tut",
  "version": "1.0.0",
  "description": "Tutorial angulara 2",
  "scripts": {
    "tsc": "./node_modules/.bin/tsc",
    "tsc:w": "./node_modules/.bin/tsc -w",
    "serve": "./node_modules/.bin/live-server --port=8080 --mount=/node_modules:./node_modules --ignorePattern=.ts  --entry-file=index.html --proxy=/api:http://localhost:8090/api",
    "go": "concurrent \"npm run tsc:w\" \"npm run serve\" "
  },
  "license": "MIT",
  "dependencies": {
    "angular2": "2.0.0-beta.17",
    "systemjs": "0.19.20",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "^0.6.10"
  },
  "devDependencies": {
    "concurrently": "^1.0.0",
    "grunt": "^0.4.5",
    "grunt-connect-proxy": "^0.2.0",
    "grunt-contrib-compass": "^1.0.4",
    "grunt-contrib-connect": "^1.0.2",
    "grunt-contrib-sass": "^0.9.2",
    "grunt-contrib-watch": "^0.6.1",
    "gulp": "^3.9.1",
    "gulp-webserver": "^0.9.1",
    "live-server": "^1.0.0",
    "proxy-middleware": "^0.15.0",
    "typescript": "^1.7.3"
  }
}
